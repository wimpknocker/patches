+        memcpy(&extraData[8], &extradata_le, 4);
+
+        int32_t height_le = U32LE_AT((uint8_t *)&height);
+        memcpy(&extraData[12], &height_le, 4);
+
+        int32_t width_le = U32LE_AT((uint8_t *)&width);
+        memcpy(&extraData[16], &width_le, 4);
+
